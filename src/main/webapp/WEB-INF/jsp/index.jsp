            <section id="contact-us">
                <div class="container">
                    <header class="text-center margin-bottom-60">
                        <h1 class="weight-300">Get In Touch</h1>
                        <h2 class="weight-300 letter-spacing-1 size-13"><span>HAVE FEEDBACK, IDEAS OR THOUGHTS ABOUT OUR SERVICES?</span></h2>
                    </header>

                    <p>Contact us anytime, weâ€™ll get back to you within 24 hours</p>
                    <form:form id="contactForm" action="/contactUs" method="post" commandName="contactForm" modelAttribute="contactForm" class="validate">
						<fieldset>
							<div class="row">
								<div class="form-group">
									<div class="col-md-6">
										<label for="contact:name">Full Name *</label>
										<input required="required" maxlength="255" type="text" value="" class="form-control" name="name" id="contact:name">
									</div>
									<div class="col-md-6">
										<label for="contact:email">Email Address *</label>
										<input required="required" maxlength="255" type="email" value="" class="form-control" name="email" id="contact:email">
									</div>
								</div>
							</div>
							<div class="row">
								<div class="form-group">
									<div class="col-md-12">
										<label for="contact:subject">Subject *</label>
										<input required="required" maxlength="255" type="text" value="" class="form-control" name="subject" id="contact:subject">
									</div>
								</div>
							</div>
							<div class="row">
								<div class="form-group">
									<div class="col-md-12">
										<label for="contact:message">Message *</label>
										<textarea required="required" maxlength="1000" rows="8" class="form-control" name="message" id="contact:message"></textarea>
									</div>
								</div>
							</div>

						</fieldset>

						<div class="row">
							<div class="col-md-12">
								<button type="submit" class="btn btn-primary"><i class="fa fa-check"></i> SEND MESSAGE</button>
							</div>
						</div>
					</form:form>
				</div>
            </section>


